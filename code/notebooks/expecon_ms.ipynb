    "# Bonferroni-corrected significance level\n",
    "alpha_level = uncorrected_alpha_level / num_comparisons\n",
    "\n",
    "# Create a figure with two rows and two columns\n",
    "fig, axes = plt.subplots(2, 2, figsize=(10, 8))\n",
